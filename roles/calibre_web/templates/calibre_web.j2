#!/bin/ksh
#
# {{ ansible_managed }}
#

daemon="/usr/local/libexec/calibre_web-launcher"
daemon_user="_calibre-web-runner"
daemon_logger="daemon.info"

. /etc/rc.d/rc.subr

pexp="/var/calibre/calibre-web/bin/python([0-9].[0-9]){0,1} /var/calibre/calibre-web/bin/cps"

rc_stop_signal=KILL
rc_reload=NO
rc_bg=YES

rc_cmd $1
